@model Association
<h1 class="text-center mb-5" style="display:inline-block;">@ViewBag.Category.Name</h1>
<a class="btn btn-primary" href="/categories" style="float:right;">Home</a>
<div class="row">
    <div class="col-md-6">
        <h3>Products</h3>
        @{
            foreach(var c in @ViewBag.AllProductsOfCategories.Products)
            {
                <p> -@c.Product.Name</p>
            }
        }
    </div>
    <div class="col-md-6">
        <partial name="addProductToCategoryPartial"/>
    </div>
</div>
